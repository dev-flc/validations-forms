{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      /* Store */
      "Actions/*": ["src/store/actions/*"],
      /* Componets */
      "Components/*": ["src/components/*"],
      /* Store */
      "Config/*": ["src/store/config/*"],
      /* constants */
      "Constants/*": ["src/constants/*"],
      /* Containers */
      "Containers/*": ["src/containers/*"],
      /* assets */
      "Fonts/*": ["src/assets/fonts/*"],
      /* Hook */
      "Hook/*": ["src/hook/*"],
      /* assets */
      "Images/*": ["src/assets/images/*"],
      /* Store */
      "Reducers/*": ["src/store/reducers/*"],
      /* Routers */
      "Routers/*": ["src/routers/*"],
      /* Store */
      "Selectors/*": ["src/store/selectors/*"],
      "Store/*": ["src/store/*"],
      /* assets */
      "Styles/*": ["src/assets/styles/*"],
      /* Utils */
      "Utils/*": ["src/utils/*"],
      /* View */
      "View/*": ["src/view/*"]
    }
  },
  "excluir": [
    "node_modules",
    "**/node_modules/*",
    "public",
    "**/public/*",
    "webpack",
    "**/webpack/*",
    ".husky",
    "**/.husky/*"
  ]
}
